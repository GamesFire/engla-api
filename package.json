{
  "name": "engla-api",
  "version": "1.0.0",
  "description": "Production-ready RESTful API for Engla â€” a short-term rental marketplace in England. Built with Node.js, Express, TypeScript, PostgreSQL, and Clean Architecture principles.",
  "exports": "./dist/entrypoint.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon",
    "build": "tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "start": "node dist/entrypoint.js",
    "clean": "rm -rf dist",
    "prepare": "husky",
    "db:migrate": "knex migrate:latest",
    "db:migrate:make": "knex migrate:make -x ts",
    "db:migrate:down": "knex migrate:rollback",
    "db:seed": "knex seed:run",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GamesFire/engla-api.git"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "express",
    "rest-api",
    "postgresql",
    "clean-architecture",
    "booking-system",
    "marketplace",
    "england",
    "backend"
  ],
  "author": "Lobix",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/GamesFire/engla-api/issues"
  },
  "homepage": "https://github.com/GamesFire/engla-api#readme",
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@bugsnag/js": "^8.8.0",
    "@bugsnag/plugin-express": "^8.8.0",
    "auth0": "^5.2.0",
    "bullmq": "^5.66.7",
    "cloudinary": "^2.9.0",
    "connection-string": "^5.0.0",
    "cors": "^2.8.6",
    "dayjs": "^1.11.19",
    "decimal.js": "^10.6.0",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "express-jwt": "^8.5.1",
    "inversify": "^7.11.0",
    "ioredis": "^5.9.2",
    "jwks-rsa": "^3.2.2",
    "knex": "^3.1.0",
    "multer": "^2.0.2",
    "node-cron": "^4.2.1",
    "objection": "^3.1.5",
    "pg": "^8.17.2",
    "reflect-metadata": "^0.2.2",
    "stripe": "^20.2.0",
    "ulid": "^3.0.2",
    "winston": "^3.19.0",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@trivago/prettier-plugin-sort-imports": "^6.0.2",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/multer": "^2.0.0",
    "@types/node": "^25.0.10",
    "@types/node-cron": "^3.0.11",
    "@vitest/coverage-v8": "^4.0.18",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-unused-imports": "^4.3.0",
    "globals": "^17.1.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "nodemon": "^3.1.11",
    "prettier": "^3.8.1",
    "supertest": "^7.2.2",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.1",
